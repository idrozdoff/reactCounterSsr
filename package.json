{
  "name": "reactCounterSsr",
  "version": "1.0.0",
  "description": "Server-Side-Rendering counter",
  "main": "index.js",
  "repository": "git@github.com:idrozdoff/reactCounterSsr.git",
  "author": "Igor Drozdov",
  "license": "MIT",
  "private": false,
  "scripts": {
    "start": "npm run pack && npm run startProd",
    "startProd": "NODE_ENV=production node index.js",
    "pack": "webpack --mode production --config webpack.config.js",
    "startDev": "npm run packDev && node index.js",
    "packDev": "webpack --mode development --config webpack.config.js",
    "packAnalyze": "NODE_ENV=analyse ./node_modules/.bin/webpack --config webpack.config.js -p"
  },
  "dependencies": {
    "@babel/cli": "^7.1.5",
    "@babel/core": "^7.1.6",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/preset-env": "^7.1.6",
    "@babel/preset-react": "^7.0.0",
    "@babel/register": "^7.0.0",
    "@material-ui/core": "^4.5.2",
    "@redux-saga/simple-saga-monitor": "^1.1.0",
    "babel-loader": "^8.0.4",
    "babel-plugin-dynamic-import-node": "^2.3.0",
    "babel-plugin-root-import": "^6.1.0",
    "express": "^4.16.4",
    "jss": "^10.0.0",
    "react": "^16.8.0",
    "react-dom": "^16.8.0",
    "react-helmet": "^5.2.0",
    "react-jss": "^10.0.0",
    "react-loadable": "^5.5.0",
    "react-redux": "^7.1.1",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "redux": "^4.0.4",
    "redux-logger": "^3.0.6",
    "redux-saga": "^1.1.1",
    "webpack": "^4.26.1",
    "webpack-cli": "^3.1.2"
  },
  "devDependencies": {
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/plugin-proposal-decorators": "^7.6.0",
    "redux-devtools": "^3.5.0",
    "redux-devtools-dock-monitor": "^1.1.3",
    "redux-devtools-log-monitor": "^1.4.0",
    "webpack-bundle-analyzer": "^3.6.0"
  }
}
